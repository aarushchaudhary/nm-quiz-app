   {
     "name": "secure-quiz-browser",
     "version": "1.0.0",
     "description": "A lightweight lockdown browser for the NMIMS Quiz App.",
     "main": "main.js",
     "scripts": {
       "start": "electron .",
       "dist": "electron-builder --win"
     },
     "author": "Aarush Chaudhary",
     "license": "MIT",
     "devDependencies": {
       "electron": "^28.0.0",
       "electron-builder": "^24.6.4"
     },
     "build": {
       "appId": "com.nmims.securequizapp",
       "productName": "NMIMS Secure Quiz App",
       "win": {
         "target": "nsis",
         "icon": "build/icon.ico"
       },
       "nsis": {
         "oneClick": false,
         "allowToChangeInstallationDirectory": true
       }
     }
   }
   

